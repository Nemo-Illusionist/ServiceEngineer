				<ResourceDictionary Source="Style/CommonStyle.xaml" />
				<ResourceDictionary Source="Style/ListViewsItemStyle.xaml" />
				<ResourceDictionary Source="Style/TextBlockStyle.xaml" />
				<ResourceDictionary Source="Style/TextBoxStyle.xaml" />
				<ResourceDictionary Source="Style/PasswordBoxStyle.xaml" />
				<ResourceDictionary Source="Style/CheckBoxStyle.xaml" />
				<ResourceDictionary Source="Style/ToggleSwitchStyle.xaml" />
				<ResourceDictionary Source="Style/LineStyle.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>

</prismUnity:PrismUnityApplication>
